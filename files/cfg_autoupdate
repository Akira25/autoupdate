config 'autoupdate' 'backup'
	option 'client_name' 'complete-hostname'
	option 'client_user' 'user'
	option 'client_path' '/complete/path/to/your/backups/'

config 'autoupdate' 'router'
	option 'router' '0'
	option 'type' '0'

config 'autoupdate' 'automode'
	option 'automode' 'false'
	option 'branch' 'release'

config 'autoupdate' 'internal_settings'
	option 'path_bin' '/tmp/sysupgrade.bin'
        option 'path_json' '/tmp/router.json'
        option 'path_bak' '/tmp/backup.tar.gz'
        option 'json_link_release' 'http://freifunk.pcfw.eu/router_release.json'
        option 'json_link_testing' 'http://freifunk.pcfw.eu/router_testing.json'
        option 'json_link_dev' 'http://freifunk.pcfw.eu/router_development.json'
        option 'log_file' '/root/autoupdate.log'

config 'autoupdate' 'upgrade_date'
	option 'last_upgr' '19700101'

